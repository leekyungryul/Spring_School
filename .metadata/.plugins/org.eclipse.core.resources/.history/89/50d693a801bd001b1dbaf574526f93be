package com.google.lkr;

public class Student {
	int idx;
	String name;
	int midScore;
	int finScore;
	String date;
	
	Student(){
		
	}
	Student(String name, int midScore, int finScore, String date){
		this.name = name;
		this.midScore = midScore;
		this.finScore = finScore;
		this.date = date;
	}
	Student(int idx, String name, int midScore, int finScore, String date){
		this.idx = idx; 
		this.name = name;
		this.midScore = midScore;
		this.finScore = finScore;
		this.date = date;
	}
	Student(int idx, String name, int midScore, int finScore){
		this.idx = idx; 
		this.name = name;
		this.midScore = midScore;
		this.finScore = finScore;
	}
}
